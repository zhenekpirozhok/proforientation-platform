{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest",
    "test:e2e": "playwright test",
    "postinstall": "npm run gen:api",
    "prebuild": "npm run gen:api",
    "gen:api:types": "openapi-typescript ../documentation/api-docs/reference/openapi.yaml -o src/shared/api/generated/schema.d.ts",
    "gen:api:client": "orval --config orval.config.ts",
    "gen:api": "npm run gen:api:types && npm run gen:api:client"
  },
  "dependencies": {
    "next": "16.0.10",
    "next-intl": "^4.6.1",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tanstack/react-query": "^5.90.12",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "openapi-typescript": "7.10.1",
    "orval": "^7.13.2",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "zustand": "^5.0.9"
  }
}
